{
    "name": "gTerminal",
    "modules": {
        "github": {
            "name": "GitHub",
            "description": "GitHub related commands",
            "script": "https://raw.githubusercontent.com/gTerminal-project/modules/main/module/github.js",
            "commands": {
                "gh": {
                    "description": "Open a specific GitHub resource"
                },
                "gist": {
                    "description": "Open a specific gist or account"
                }
            }
        },
        "google": {
            "name": "Google",
            "description": "Google related commands",
            "script": "https://raw.githubusercontent.com/gTerminal-project/modules/main/module/google.js",
            "commands": {
                "google": {
                    "description": "Search on google"
                }
            },
            "aliases": {
                "goog": {
                    "command": "google"
                }
            }
        },
        "openai": {
            "name": "OpenAI",
            "description": "OpenAI related commands",
            "script": "https://raw.githubusercontent.com/gTerminal-project/modules/main/module/openai.js",
            "commands": {
                "chatgpt": {
                    "description": "Open a specific ChatGPT chat or ChatGPT"
                }
            }
        },
        "shortlinks": {
            "name": "Shortlink Commands",
            "description": "Provides commands to generate shortlinks on is.gd and v.gd",
            "script": "https://raw.githubusercontent.com/gTerminal-project/modules/main/module/shortlinks.js",
            "commands": {
                "short": {
                    "description": "Shorten a link on is.gd"
                },
                "shortv": {
                    "description": "Shorten a link on v.gd"
                }
            }
        },
        "twitch": {
            "name": "Twitch",
            "description": "Search People and Categories on Twitch",
            "script": "https://raw.githubusercontent.com/gTerminal-project/modules/main/module/twitch.js",
            "commands": {
                "tw": {
                    "description": "Open Twitch or search on it"
                },
                "twac": {
                    "description": "Show a twitch profile"
                },
                "twcat": {
                    "description": "Show a twitch category"
                },
                "twcat-all": {
                    "description": "List availible twitch categories"
                }
            }
        },
        "webdev": {
            "name": "WebDev Tools",
            "description": "Tools for web developers",
            "script": "https://raw.githubusercontent.com/gTerminal-project/modules/main/module/webdev.js",
            "commands": {
                "pen": {
                    "description": "Start a new codepen"
                },
                "keycode": {
                    "description": "Open keycode.info"
                }
            }
        },
        "whatsapp": {
            "name": "WhatsApp",
            "description": "Open WhatsApp Web with a simple command",
            "script": "https://raw.githubusercontent.com/gTerminal-project/modules/main/module/whatsapp.js",
            "commands": {
                "whatsapp": {
                    "description": "Open WhatsApp Web"
                }
            },
            "aliases": {
                "wa": {
                    "command": "whatsapp"
                }
            }
        },
        "you": {
            "name": "You.com",
            "description": "You.com related commands",
            "script": "https://raw.githubusercontent.com/gTerminal-project/modules/main/module/you.js",
            "commands": {
                "you": {
                    "description": "Search with youchat"
                },
                "you-search": {
                    "description": "Search on You.com"
                },
                "chat": {
                    "description": "Open a specific you-chat session or you-chat"
                }
            },
            "aliases": {
                "u": {
                    "command": "you"
                },
                "us": {
                    "command": "you-search"
                }
            },
            "missing_command_helpers": [
                {
                    "description": "Search with You.com's YouChat",
                    "command": "u %full%"
                }
            ]
        },
        "yt": {
            "name": "YouTube",
            "description": "YouTube related commands",
            "script": "https://raw.githubusercontent.com/gTerminal-project/modules/main/module/youtube.js",
            "commands": {
                "youtube": {
                    "description": "Search with youchat"
                }
            },
            "aliases": {
                "yt": {
                    "command": "youtube"
                }
            }
        }
    }
}
